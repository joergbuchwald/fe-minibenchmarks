                <secondary_variable internal_name="liquid_density" output_name="liquid_density"/>
            </secondary_variables>
            <initial_stress>nonequilibrium_stress</initial_stress>
            <specific_body_force>0 0</specific_body_force>
            <mass_lumping>false</mass_lumping>
            <apply_body_force_for_deformation>false</apply_body_force_for_deformation>
