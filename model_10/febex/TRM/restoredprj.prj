                        </property>
                        <property>
                            <name>thermal_diffusion_enhancement_factor            </name>
                            <type>Constant</type>
                            <value>1.0</value>
                        </property>
                        <property>
                            <name>latent_heat</name>
                            <type>LinearWaterVapourLatentHeat</type>
                        </property>
                        <property>
                            <name>thermal_conductivity</name>
                            <type>Constant</type>
                            <value>0.6</value>
                        </property>
                    </properties>
                </phase>
                <phase>
                    <type>Solid</type>
                    <properties>
                        <property>
                            <name>density</name>
                            <type>Constant</type>
                            <value>2690</value>
                        </property>
                        <property>
                            <name>specific_heat_capacity</name>
                            <type>Constant</type>
                            <value>995</value>
                        </property>
                        <property>
                            <name>thermal_expansivity</name>
                            <type>Constant</type>
                            <value>1.7e-5</value>
                        </property>
                    </properties>
                </phase>
            </phases>
            <properties>
                <property>
