                <poissons_ratio>nuHeater</poissons_ratio>
            </constitutive_relation>
            <constitutive_relation id="1">
                <type>LinearElasticIsotropic</type>
                <youngs_modulus>EGBM</youngs_modulus>
                <poissons_ratio>nuGBM</poissons_ratio>
            </constitutive_relation>
            <constitutive_relation id="2">
                <type>LinearElasticIsotropic</type>
                <youngs_modulus>EShotcrete</youngs_modulus>
                <poissons_ratio>nuShotcrete</poissons_ratio>
            </constitutive_relation>
            <constitutive_relation id="3">
                <type>LinearElasticIsotropic</type>
                <youngs_modulus>EEDZ</youngs_modulus>
                <poissons_ratio>nuEDZ</poissons_ratio>
            </constitutive_relation>
            <constitutive_relation id="4">
