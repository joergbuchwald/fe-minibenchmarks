    </media>
    <time_loop>
        <processes>
            <process ref="THERMO_RICHARDS_MECHANICS">
                <nonlinear_solver>basic_newton</nonlinear_solver>
                <convergence_criterion>
                    <type>PerComponentDeltaX</type>
                    <norm_type>NORM2</norm_type>
                    <abstols>1e-10 1e-10 1e-10 1e-10</abstols>
                    <reltols>1e-10 1e-10 1e-10 1e-10</reltols>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <time_stepping>
                    <type>FixedTimeStepping</type>
                    <t_initial>-31557600</t_initial>
                    <t_end>473040000</t_end>
                    <timesteps>
                        <pair>
                            <repeat>10</repeat>
                            <delta_t>86400</delta_t>
                        </pair>
                        <pair>
                            <repeat>9</repeat>
                            <delta_t>864000</delta_t>
                        </pair>
                        <pair>
                            <repeat>5</repeat>
                            <delta_t>1728000</delta_t>
                        </pair>
                        <pair>
                            <repeat>10</repeat>
                            <delta_t>3456000</delta_t>
                        </pair>
                    </timesteps>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>TRM_run_10</prefix>
            <timesteps>
                <pair>
                    <repeat>200</repeat>
                    <each_steps>2</each_steps>
                </pair>
            </timesteps>
            <output_iteration_results>false</output_iteration_results>
            <variables>
                <variable>pressure</variable>
                <variable>temperature</variable>
                <variable>saturation</variable>
                <variable>displacement</variable>
                <variable>sigma</variable>
                <variable>epsilon</variable>
                <variable>velocity</variable>
                <variable>liquid_density</variable>
            </variables>
        </output>
    </time_loop>
    <parameters>
