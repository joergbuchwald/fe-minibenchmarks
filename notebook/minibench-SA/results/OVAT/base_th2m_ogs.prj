    <time_loop>
        <processes>
            <process ref="TH2M">
                <nonlinear_solver>basic_newton</nonlinear_solver>
                <convergence_criterion>
                    <type>PerComponentDeltaX</type>
                    <!--type>Residual</type-->
                    <norm_type>NORM2</norm_type>
                    <!--abstol>7e-5</abstol-->
                    <abstols>1e-8 1e-2 1e-8 1e-10 1e-10</abstols>
                    <reltols>1e-7 1e-7 1e-7 1e-7 1e-7</reltols>
                </convergence_criterion>
                <time_discretization>
                    <type>BackwardEuler</type>
                </time_discretization>
                <time_stepping>
                    <type>IterationNumberBasedTimeStepping</type>
                    <t_initial>-864000.000000</t_initial>
                    <!--t_initial>1606650431.102651</t_initial-->
                    <!--t_end>47304000000</t_end-->
                    <!-- original: t_end>1.2e10</t_end-->
                    <t_end>1.0e9</t_end>
                    <initial_dt>864000</initial_dt>
                    <minimum_dt>86400.0</minimum_dt>
                    <maximum_dt>8640000</maximum_dt>
                    <!--orig: maximum_dt>4.32e6</maximum_dt-->
                    <number_iterations>1   2   10 20     30  40  50    54  </number_iterations>
                    <multiplier>    10.0   5   2 1.5  1.2  1.0 1.0  0.5 </multiplier>
                </time_stepping>
            </process>
        </processes>
        <output>
            <type>VTK</type>
            <prefix>base_th2m</prefix>
            <!--suffix>_ts_{:timestep}_t_{:time}_i_{:iteration}</suffix-->
            <timesteps>
                <pair>
                    <repeat>200</repeat>
                    <each_steps>5</each_steps>
                </pair>
            </timesteps>
            <output_iteration_results>false</output_iteration_results>
